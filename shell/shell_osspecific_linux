
alias nvconf='cd ~/.config/nvim/lua/ && nvim ~/.config/nvim/lua/ '

# window manager stuff
# . on linux
alias wm='cd ~/.dotfiles/windowmanager/linux && nvim i3/config polybar/config.ini rofi/config.rasi'

# opening apps
alias wezterm='flatpak run org.wezfurlong.wezterm'

export SUDO_EDITOR='/home/ruud/.local/bin/nvim'
